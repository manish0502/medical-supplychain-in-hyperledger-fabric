channels
./bin/configtxgen -profile <Channel_name> -outputCreateChannelTx ./channel-artifacts/<channel-name.tx> -channelID <channel_id>
./bin/configtxgen -profile klmchannel -outputCreateChannelTx ./channel-artifacts/klmchannel.tx -channelID SampleConsortium
./bin/configtxgen -profile klmchannel -outputCreateChannelTx ./channel-artifacts/klmchannel.tx -channelID klm-channel
